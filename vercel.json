{
  "version": 2,
  "builds": [
    {
      "src": "api/model.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",  
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "game_map.pkl",
          "model.pkl",
          "label_encoder.pkl"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/model.py"
    }
  ]
}
